{"app":"<div id=\"config-app\" style=\"height: 800px;\">\n    <textarea \n        style=\"width: 100%;\" \n        rows=\"5\" \n        name=\"jform[params][config]\" \n        id=\"jform_params_config\">{{value}}<\/textarea>\n    <div class=\"filter-wrapper\">\n        <filter-app><\/filter-app>\n        <filter-add><\/filter-add>\n        <hr style=\"clear: both;\">\n        <div class=\"sidebar\" style=\"width: 30%; float: left;\">\n            <filter-list><\/filter-list>\n        <\/div>\n        <div class=\"config\" style=\"width: 70%; float: left;\">\n            <filter-config><\/filter-config>\n        <\/div>\n    <\/div>\n    <hr style=\"clear: both;\">\n    <div class=\"sorting-wrapper\">\n        <h4>ordering<\/h4>\n    <\/div>\n<\/div>","filter-add":"<div class=\"filter-add\">\n    <h4>Filter Add<\/h4>\n    <el-button \n        type=\"primary\" \n        @click=\"dialogVisible = true\">Add field<\/el-button>\n\n    <el-dialog\n        title=\"Fields\"\n        :visible.sync=\"dialogVisible\"\n        width=\"30%\"\n        class=\"field-dialog\">\n        <div class=\"base-fields\">\n            <h4>Base Fields<\/h4>\n            <div class=\"base-field-list\">\n                <el-button \n                    v-for=\"field in baseFields\" \n                    :key=\"field.name\"\n                    :type=\"added.indexOf(field.name) > - 1 ? 'primary' : ''\"\n                    @click=\"addFilter(field)\">\n                    {{field.title}}\n                <\/el-button>\n            <\/div>\n        <\/div>\n        \n        <div class=\"custom-fields\">\n            <h4>Custom Fields<\/h4>\n            <div class=\"categories\">\n                <el-select v-model=\"categoryId\" placeholder=\"Select Category\">\n                    <el-option\n                        v-for=\"cat in categories\"\n                        :key=\"cat.id\"\n                        :label=\"cat.treeName\"\n                        :value=\"cat.id\">\n                 