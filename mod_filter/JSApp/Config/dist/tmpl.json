{"app":"<div id=\"config-app\">\n    <textarea \n        style=\"width: 100%;\" \n        rows=\"5\" \n        name=\"jform[params][config]\" \n        id=\"jform_params_config\">{{value}}<\/textarea>\n    <hr>\n    <div class=\"filter-wrapper\">\n        <filter-app><\/filter-app>\n        <filter-add><\/filter-add>\n        <hr style=\"clear: both;\">\n        <div class=\"sidebar\" style=\"width: 30%; float: left;\">\n            <filter-list><\/filter-list>\n        <\/div>\n        <div class=\"config\" style=\"width: 70%; float: left;\">\n            <filter-config><\/filter-config>\n        <\/div>\n    <\/div>\n    <hr style=\"clear: both;\">\n    <div class=\"sorting-wrapper\">\n        <h4>ordering<\/h4>\n    <\/div>\n<\/div>","filter-add":"<div class=\"filter-add\">\n    <h4>Filter Add<\/h4>\n    <el-button \n        type=\"primary\" \n        @click=\"dialogVisible = true\">Add field<\/el-button>\n\n    <el-dialog\n        title=\"Fields\"\n        :visible.sync=\"dialogVisible\"\n        width=\"30%\"\n        class=\"field-dialog\">\n        <div class=\"base-fields\">\n            <h4>Base Fields<\/h4>\n            <div class=\"base-field-list\">\n                <el-button \n                    v-for=\"field in baseFields\" \n                    :key=\"field.name\"\n                    :type=\"added.indexOf(field.name) > - 1 ? 'primary' : ''\"\n                    @click=\"addFilter(field)\">\n                    {{field.title}}\n                <\/el-button>\n            <\/div>\n        <\/div>\n        \n        <div class=\"custom-fields\">\n            <h4>Custom Fields<\/h4>\n            <div class=\"categories\">\n                <el-select v-model=\"categoryId\" placeholder=\"Select Category\">\n                    <el-option\n                        v-for=\"cat in categories\"\n                        :key=\"cat.id\"\n                        :label=\"cat.treeName\"\n                        :value=\"cat.id\">\n                    <\/el-option>\n                <\/el-select>\n            <\/div>\n            <div class=\"custom-field-list\">\n                <el-button \n                    v-for=\"field in customFields\" \n                    :key=\"field.name\"\n                    :type=\"added.indexOf(field.name) > - 1 ? 'primary' : ''\"\n                    @click=\"addFilter(field)\">\n                    {{field.title}}\n                <\/el-button>\n            <\/div>\n        <\/div>\n    <\/el-dialog>\n<\/div>","filter-app":"<div class=\"filter-app\">\n    <h4>Filter App<\/h4>\n    <div class=\"filter-app-selector\">\n        <el-select \n            v-model=\"appid\" \n            placeholder=\"Select App\"\n            @change=\"changeFilterApp\">\n            <el-option\n                v-for=\"app in apps\"\n                :key=\"app.id\"\n                :label=\"app.title\"\n                :value=\"app.id\">\n            <\/el-option>\n        <\/el-select>\n    <\/div>\n<\/div>","filter-config-text":"<div class=\"filter-config-text\">\n    <b>Title<\/b>\n    <el-input\n        placeholder=\"Title\"\n        :value=\"item.config.title\"\n        @input=\"updateTitle\">\n    <\/el-input>\n\n    <b>Max Length<\/b>\n    <el-input\n        placeholder=\"Max Length\"\n        :value=\"item.config.maxlength\"\n        @input=\"updateMaxLength\">\n    <\/el-input>\n<\/div>","filter-config":"<div class=\"filter-config-wrapper\">\n    <h4>Filter Config<\/h4>\n    <div v-if=\"item\" class=\"filter-config-inner\">\n        <b>Choose template<\/b>\n        <el-select \n            :value=\"item.template\"\n            @change=\"changeTemplate\"\n            placeholder=\"Select Type\">\n            <el-option\n                v-for=\"component in item.components\"\n                :key=\"component.template\"\n                :label=\"component.title\"\n                :value=\"component.template\">\n            <\/el-option>\n        <\/el-select>\n        <filter-config-text \n            v-if=\"item.type === 'text'\"\n            :key=\"item.id\"\n            :item=\"item\">\n        <\/filter-config-text>\n    <\/div>\n<\/div>","filter-list":"<div class=\"filter-list\">\n    <h4>Filter List<\/h4>\n        <div v-for=\"item in list\" class=\"filter-item\">\n            <el-button \n                :type=\"active === item.id ? 'primary' : ''\"\n                @click=\"setActive(item.id)\">\n                {{item.group}}.{{item.title}}\n            <\/el-button>\n            <a @click.prevent=\"duplicateFilter(item)\" href=\"#\">Duplicate<\/a>\n            <a @click.prevent=\"deleteFilter(item.id)\" href=\"#\">Delete<\/a>\n        <\/div>\n<\/div>","store":""}